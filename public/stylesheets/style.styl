//imports

@import 'font.css'
@import nib
@import mixins






// start this shit

global-reset()

*, *:before, *:after
	box-sizing border-box


body
	background white
	font()
	font-size 100%

.wrapper
	padding 2%


nav.main
	fixed bottom
	width 100%
	background pink
	color white
	padding 1em 0
	clearfix()
	h1
		font-size 100px
		float left
	.shuffle
		float right
		i
			font-size 100px




.mannequin	
	margin 0 auto
	.modelHead
		width 100px
		height 100px
		margin 0 auto
		background blue
		.face
			width 100%
			height 100%
			display block
			margin 0 auto
			background white
			border-radius 100px
